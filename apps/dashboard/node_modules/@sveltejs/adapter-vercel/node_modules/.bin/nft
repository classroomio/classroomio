#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/@vercel+nft@0.23.1/node_modules/@vercel/nft/out/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/@vercel+nft@0.23.1/node_modules/@vercel/nft/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/@vercel+nft@0.23.1/node_modules/@vercel/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/@vercel+nft@0.23.1/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/@vercel+nft@0.23.1/node_modules/@vercel/nft/out/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/@vercel+nft@0.23.1/node_modules/@vercel/nft/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/@vercel+nft@0.23.1/node_modules/@vercel/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/@vercel+nft@0.23.1/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@vercel+nft@0.23.1/node_modules/@vercel/nft/out/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../@vercel+nft@0.23.1/node_modules/@vercel/nft/out/cli.js" "$@"
fi
