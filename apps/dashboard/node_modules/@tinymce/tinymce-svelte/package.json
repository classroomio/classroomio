{
  "name": "@tinymce/tinymce-svelte",
  "version": "1.0.1",
  "description": "TinyMCE Svelte Component",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "https://github.com/tinymce/tinymce-svelte"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "svelte": "dist/index.js",
  "types": "dist/ts/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "validate": "svelte-check",
    "lint": "eslint src/main/**/*.{ts,svelte}",
    "test": "echo 'No tests hooked up yet'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "yarn storybook-to-ghpages --source-branch=main",
    "prepublishOnly": "yarn run build"
  },
  "keywords": [
    "tinymce",
    "svelte",
    "component"
