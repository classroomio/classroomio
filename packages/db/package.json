{
  "name": "@cio/db",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist tsconfig.tsbuildinfo && tsc && tsc-alias",
    "dev": "tsc --watch",
    "db": "drizzle-kit",
    "move-users": "tsx src/scripts/supabase-to-betterauth.ts",
    "pull": "drizzle-kit pull --config drizzle.config.ts",
    "generate-types": "tsx scripts/generate-types.ts && pnpm format",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@cio/email": "workspace:*",
    "@cio/tsconfig": "workspace:*",
    "@cio/utils": "workspace:*",
    "better-auth": "^1.4.1",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.7",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@types/node": "^24.8.1",
    "dotenv": "^17.2.3",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.9.3"
  }
}
