{
  "name": "@cio/ui",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "generate-css": "pnpx @tailwindcss/cli -i ./src/index.css -o ./src/output.css --minify",
    "build:watch": "pnpm generate-css --watch",
    "dev": "vite build --watch",
    "sync:exports": "node scripts/sync-exports.js && prettier --write vite.config.js",
    "build": "pnpm run sync:exports && vite build"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/js/index.es",
      "require": "./dist/js/index.cjs"
    },
    "./button": {
      "types": "./src/base/button/index.ts",
      "import": "./dist/js/button.es",
      "require": "./dist/js/button.cjs"
    },
    "./dropdown-menu": {
      "types": "./src/base/dropdown-menu/index.ts",
      "import": "./dist/js/dropdown-menu.es",
      "require": "./dist/js/dropdown-menu.cjs"
    },
    "./input": {
      "types": "./src/base/input/index.ts",
      "import": "./dist/js/input.es",
      "require": "./dist/js/input.cjs"
    },
    "./popover": {
      "types": "./src/base/popover/index.ts",
      "import": "./dist/js/popover.es",
      "require": "./dist/js/popover.cjs"
    },
    "./skeleton": {
      "types": "./src/base/skeleton/index.ts",
      "import": "./dist/js/skeleton.es",
      "require": "./dist/js/skeleton.cjs"
    },
    "./tooltip": {
      "types": "./src/base/tooltip/index.ts",
      "import": "./dist/js/tooltip.es",
      "require": "./dist/js/tooltip.cjs"
    },
    "./editor": {
      "types": "./src/custom/editor/ui/editor.ts",
      "import": "./dist/js/editor.es",
      "require": "./dist/js/editor.cjs"
    }
  },
  "devDependencies": {
    "@internationalized/date": "^3.8.1",
    "@lucide/svelte": "^0.544.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.5.27",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@tailwindcss/typography": "^0.5.18",
    "@tailwindcss/vite": "^4.1.13",
    "clsx": "^2.1.1",
    "svelte": "^5.39.11",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "vite": "^7.1.12"
  },
  "dependencies": {
    "@aarkue/tiptap-math-extension": "^1.4.0",
    "@lucide/svelte": "^0.546.0",
    "@tiptap/core": "^3.7.1",
    "@tiptap/extension-bubble-menu": "^3.7.1",
    "@tiptap/extension-code-block-lowlight": "^3.7.1",
    "@tiptap/extension-highlight": "^3.7.1",
    "@tiptap/extension-image": "^3.7.1",
    "@tiptap/extension-list": "^3.7.1",
    "@tiptap/extension-subscript": "^3.7.1",
    "@tiptap/extension-superscript": "^3.7.1",
    "@tiptap/extension-table": "^3.7.1",
    "@tiptap/extension-text-align": "^3.7.1",
    "@tiptap/extension-text-style": "^3.7.1",
    "@tiptap/extension-typography": "^3.7.1",
    "@tiptap/extensions": "^3.7.1",
    "@tiptap/pm": "^3.7.1",
    "@tiptap/starter-kit": "^3.7.1",
    "@tiptap/suggestion": "^3.7.1",
    "katex": "^0.16.22",
    "lowlight": "^3.3.0",
    "mode-watcher": "^0.5.1",
    "svelte-tiptap": "^3.0.0",
    "tippy.js": "^6.3.7",
    "tiptap-extension-auto-joiner": "^0.1.3",
    "tiptap-markdown": "^0.9.0"
  },
  "peerDependencies": {
    "bits-ui": "^2.11.0"
  }
}