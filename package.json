{
  "name": "classroomio",
  "version": "0.1.13",
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "dev": "turbo prepare --concurrency 13 && turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo clean && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:pnpm": "pnpm clean && pnpm store prune",
    "format": "prettier . --write",
    "release": "standard-version",
    "dashboard:dev": "pnpm dev --filter=@cio/dashboard",
    "dashboard:dev-full": "pnpm dev --filter=@cio/dashboard --filter=@cio/ui  --filter=@cio/db --filter=@cio/utils",
    "dashboard:start": "pnpm start --filter @cio/dashboard",
    "api:dev": "pnpm dev --filter=@cio/api",
    "api:dev-full": "pnpm dev --filter=@cio/api --filter=@cio/email  --filter=@cio/db --filter=@cio/utils",
    "api:start": "pnpm start --filter @cio/api",
    "supabase:push": "pnpx supabase link --project-ref $PROJECT_ID && pnpx supabase db push --include-all",
    "storybook:dev": "pnpm dev --filter=@cio/storybook --filter=@cio/ui"
  },
  "license": "MIT",
  "dependencies": {
    "turbo": "^1.10.16"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^9.36.0",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "standard-version": "^9.5.0",
    "@cio/tsconfig": "workspace:*",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": "^20.19.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@tailwindcss/oxide"
    ],
    "overrides": {
      "zod": "^4.1.8"
    }
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
