{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**", ".svelte-kit/**", ".vercel/**"]
		},
		"lint": {
			"dependsOn": ["^build"]
		},
		"format": {"cache": true},
		"dev": {
			"dependsOn": ["^db:generate"],
			"cache": false,
			"persistent": true
		},
		"clean": { "cache": false },
		"db:generate": { "cache": false },
		"db:push": { "cache": false },
		"db:format": { "cache": false }
	}
}
